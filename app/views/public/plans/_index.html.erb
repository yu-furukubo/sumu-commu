<table class='table table-borderless table-hover'>
  <thead>
    <tr>
      <th>予定名</th>
      <th>日程</th>
      <th>場所</th>
    </tr>
  </thead>
  <tbody>
    <% plans.each do |plan| %>
      <tr class="index_tr" data-href="/public/plans/<%= plan.id %>">
        <td><%= plan.subject %></td>
        <td><%= l plan.started_at %> ~ <%= l plan.finished_at %></td>
        <td><%= plan.venue %></td>
      </tr>
    <% end %>
  </tbody>
</table>